BlinkEnde_LED.ino

void setup ( ) { // I n i t i a l i s i e r t Pin 14 (LED1) a l s output .
pinMode(14,OUTPUT);
pinMode(0,INPUT);
int i = 0;
}
//Testen des Spannungsnulldurchgangs und ZÃ¤hlen
void loop() {
        if(digitalRead(6,HIGH)){
            digitalWrite(14,HIGH);
        }
        delay(8);
        digitalWrite(14,LOW);
        i++;
    }

