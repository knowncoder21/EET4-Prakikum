BlinkEnde_LED.ino

void setup ( ) {
pinMode(1, INPUT);
pinMode(0,INPUT);
pinMode(3,OUTPUT);
int a;
int aMax;
int aMin;
}

void loop() {
         if(digitalRead(6,HIGH)){
            delay(a);
            digitalWrite(3,HIGH);
            delayMicroseconds(10);
            digitalWrite(3,LOW);
        }
        if(digitalRead(0,HIGH) && a> aMin){
            a-=50;

        }
        if(digitalRead(1, HIGH) && a< aMax){
            a+=50;
        }
        
    }

