void loop() {
        if(digitalRead(6,HIGH)){
            delay(a);
            digitalWrite(3,HIGH);
            delayMicroseconds(10);
            digitalWrite(3,LOW);
        }
        if(digitalRead(0,HIGH)){
            a-=50;

        }
        if(digitalRead(1, HIGH)){
            a+=50;
        }
