BlinkEnde_LED.ino

void setup ( ) { // I n i t i a l i s i e r t Pin 14 (LED1) a l s output .
pinMode (14,OUTPUT) ;
}
// LED1 b l i n k t im Zwe i s e kundentak t
void loop ( ) {
digitalWrite(14,HIGH) ; // LED1 an
delay (1000) ; // 1000 ms warten
digitlWrite(14,LOW) ; // LED1 aus
de ay ( 1 0 0 0 ) ; // 1000 ms warten
}
